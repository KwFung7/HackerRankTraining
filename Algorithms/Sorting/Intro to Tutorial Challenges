import java.io.*;
import java.util.*;

public class Solution {
    
    public static void main(String[] args) {
        Scanner input = new Scanner(System.in);
        int v = input.nextInt();
        int n = input.nextInt();
        int[] arr = new int[n];
        for (int i = 0; i < n ; i++) {
            arr[i] = input.nextInt();
            if (arr[i] == v) {
                System.out.println(i);
                break;
            }
        }
    }
    
}
